{
  "name": "root",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elppac/form-designable.git"
  },
  "workspaces": [
    "packages/*",
    "formily/*",
    "examples/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider && rimraf -rf packages/*/{lib,esm} && lerna run build",
    "build:docs": "dumi build",
    "clean": "lerna clean",
    "postinstall": "opencollective-postinstall",
    "lint": "eslint --ext .ts,.tsx,.js  --fix",
    "prelease": "lerna publish --dist-tag next",
    "prelease:force": "lerna publish from-package --yes --dist-tag next",
    "release": "lerna publish",
    "release:force": "lerna publish from-package --yes",
    "release:github": "ts-node scripts/release release",
    "start": "dumi dev",
    "start:basic": "yarn workspace @creatormatrix/basic-example start",
    "start:multi-workspace": "yarn workspace @creatormatrix/multi-workspace-example start",
    "start:playground": "cross-env NODE_OPTIONS=--openssl-legacy-provider && npm run start:basic",
    "start:sandbox": "yarn workspace @creatormatrix/sandbox-example start",
    "start:sandbox-multi-workspace": "yarn workspace @creatormatrix/sandbox-multi-workspace-example start",
    "test": "jest --coverage",
    "test:prod": "jest --coverage --silent",
    "test:watch": "jest --watch",
    "preversion": "npm run build && npm run lint",
    "version": "ts-node scripts/release changelog && git add -A",
    "version:alpha": "lerna version prerelease --preid alpha",
    "version:beta": "lerna version prerelease --preid beta",
    "version:major": "lerna version major",
    "version:minor": "lerna version minor",
    "version:patch": "lerna version patch",
    "version:preminor": "lerna version preminor --preid beta",
    "version:rc": "lerna version prerelease --preid rc"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --ext .ts,.tsx,.js --fix",
      "pretty-quick --staged",
      "git add"
    ],
    "*.md": [
      "pretty-quick --staged",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node ./scripts/validate-commit-msg.js"
    }
  },
  "resolutions": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "yargs": "^16.x"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2"
  },
  "devDependencies": {
    "@alifd/next": "^1.19.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^11.2.3",
    "@types/dateformat": "^3.0.1",
    "@types/fs-extra": "^8.1.0",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.6.8",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.8.2",
    "@umijs/plugin-sass": "^1.1.1",
    "antd": "^4.0.0",
    "chalk": "^2.4.2",
    "chokidar": "^2.1.2",
    "concurrently": "^4.1.0",
    "conventional-commit-types": "^2.2.0",
    "cool-path": "^1.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^5.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "dumi": "^2.4.6",
    "escape-string-regexp": "^4.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-markdown": "^2.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "execa": "^5.0.0",
    "file-loader": "^5.0.2",
    "findup": "^0.1.5",
    "fs-extra": "^7.0.1",
    "gh-release": "^5.0.1",
    "ghooks": "^2.0.4",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "immutable": "^4.0.0-rc.12",
    "istanbul-api": "^2.1.1",
    "istanbul-lib-coverage": "^2.0.3",
    "jest": "^26.0.0",
    "jest-dom": "^3.1.2",
    "jest-styled-components": "6.3.3",
    "jest-watch-lerna-packages": "^1.1.0",
    "lerna": "^4.0.0",
    "less": "^4.1.1",
    "less-loader": "^5.0.0",
    "less-plugin-npm-import": "^2.1.0",
    "lint-staged": "^8.2.1",
    "mobx": "^6.0.4",
    "mobx-react-lite": "^3.1.6",
    "onchange": "^5.2.0",
    "opencollective-postinstall": "^2.0.3",
    "param-case": "^3.0.4",
    "postcss": "^8.0.0",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^2.2.1",
    "pretty-format": "^24.0.0",
    "pretty-quick": "^3.1.0",
    "raw-loader": "^4.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.37.1",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "semver": "^7.3.5",
    "semver-regex": "^2.0.0",
    "showdown": "^1.9.1",
    "staged-git-files": "^1.1.2",
    "string-similarity": "^4.0.4",
    "style-loader": "^4.0.0",
    "styled-components": "^5.0.0",
    "ts-import-plugin": "1.6.1",
    "ts-jest": "^26.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.1.5",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "devEngines": {
    "node": "8.x || 9.x || 10.x || 11.x"
  }
}
